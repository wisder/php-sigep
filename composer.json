{
    "name": "wisder/php-sigep",
    "type": "library",
    "description": "Integração com Web Service do Correios. Gera etiquetas, consulta preços e prazos, imprime etiquetas e PLP, etc.",
    "keywords": ["correios", "sigep"],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    },
    "require": {
        "php": ">=5.4",
        "ext-soap": "*",
        "ext-mbstring": "*",
        "wisder/php-sigep-fpdf": "^1.0.0",
        "vria/nodiacritic": "^0.1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4"
    },
    "autoload": {
        "psr-0": {
            "PhpSigep": "src/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --verbose --colors=always"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Rafael Stavarengo",
            "email": "faelsta@gmail.com"
        },
        {
            "name": "Success Go",
            "email": "success.go@gaodacheng.com"
        }
    ]
}
